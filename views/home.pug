    style.
      body {
      width: 100%;
      height: 100%;
      padding: 0;
      margin: 0;
      }
      #property_checkbox label {
      margin: 0px 3px 0px 3px;
      }
      ::-webkit-scrollbar {
      -webkit-appearance: none;
      width: 8px;
      }
      ::-webkit-scrollbar-thumb {
      border-radius: 4px;
      background-color: rgba(0, 0, 0, .5);
