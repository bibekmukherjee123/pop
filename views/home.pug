  body#body.search_body
    header#site-header.site-header.search_header
      .container
        .row
          .col-md-2
            .site-logo
              a(href='/home')
                img.img-fluid(src='/user/images/logo.png', title='Logo')
              a.search_menu(href='#!')
                i.fa.fa-search(aria-hidden='true')
          .col-md-9
            .property-search-wrapper.max_screen
              form.property-search-form(action='javascript:void(0)')
                .clear-all
                ul.list-common
                  li.form-field
                    .block-container
                      .keyword-search-box
                        select#keyword.keyword(name='keyword[]', multiple='multiple', style='width: 100%')
                          // <optgroup id="property_names" label="Property Names">
                          // <optgroup id="locations" label="Locations">
                          optgroup#property_tags(label='Location Tags')
                  li.form-field
                    .property-type-box
                      input#message-1.input-area.select_box(type='button', value='Property Type')
                    #respond-1.dropdown-list.prop-type-dropdown(style='display: none')
                      .prop-type
                        #property_checkbox.property_label
                  li.form-field
                    .budget-box
                      input#message-2.input-area(type='button', value='Budget')
                    #respond-2.budget-dropdown(style='display:none')
                      .refine-budget
                        .range.range-list
                          .range-option
                            #min.range-link.range-option--min MIN
                            #max.range-link.range-option--max MAX
                          #min_show.range-min.refine-budget--range-min.dropdown-select2-container
                            select#budget_min.budget-range-min(name='budgetMin[]', style='width: 100%')
                              option(value='10000') 10 K
                              option(value='20000') 20 K
                              option(value='30000') 30 K
                              option(value='40000') 40 K
                              option(value='50000') 50 K
                              option(value='60000') 60 K
                              option(value='70000') 70 K
                              option(value='80000') 80 K
                              option(value='90000') 90 K
                              option(value='1000000') 1 Lac
                              option(value='500000') 5 Lac
                              option(value='1000000') 10 Lac
                              option(value='2000000') 20 Lac
                              option(value='2500000') 25 Lac
                              option(value='3000000') 30 Lac
                              option(value='4000000') 40 Lac
                              option(value='5000000') 50 Lac
                              option(value='6000000') 60 Lac
                              option(value='7000000') 70 Lac
                              option(value='8000000') 80 Lac
                              option(value='9000000') 90 Lac
                              option(value='10000000') 1 Cr
                              option(value='50000000') 5 Cr
                          #max_show.range-max.refine-budget--range-max.dropdown-select2-container(style='display: none')
                            select#budget_max.budget-range-max(name='budgetMax[]', style='width: 100%')
                              option(value='20000') 20 K
                              option(value='30000') 30 K
                              option(value='40000') 40 K
                              option(value='50000') 50 K
                              option(value='60000') 60 K
                              option(value='70000') 70 K
                              option(value='80000') 80 K
                              option(value='90000') 90 K
                              option(value='1000000') 1 Lac
                              option(value='500000') 5 Lac
                              option(value='1000000') 10 Lac
                              option(value='2000000') 20 Lac
                              option(value='2500000') 25 Lac
                              option(value='3000000') 30 Lac
                              option(value='4000000') 40 Lac
                              option(value='5000000') 50 Lac
                              option(value='6000000') 60 Lac
                              option(value='7000000') 70 Lac
                              option(value='8000000') 80 Lac
                              option(value='9000000') 90 Lac
                              option(value='10000000') 1 Cr
                              option(value='50000000') 5 Cr
                              option(value='100000000', selected='selected') 10 Cr
                  li.form-field.search-button
                    .search-button-wrapper
                      a(onclick='searchProperty()')
                        i.fa.fa-search(aria-hidden='true')
                .clear-all
